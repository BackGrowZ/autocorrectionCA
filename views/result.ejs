<html>
    <head>
      <meta charset="utf-8" />
      <title>Auto Correcteur Coding Accelerator</title>
      <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Zenh87qX5JnK2Jl0vWa8Ck2rdkQ2Bzep5IDxbcnCeuOxjzrPF/et3URy9Bv1WTRi" crossorigin="anonymous">
      <link href="http://<%= URL %>/terminal.css" rel="stylesheet">
    </head>
    <body>
      <div class="container">
        <div class="accordion" id="accordionPanelsStayOpenExample">
          <% Object.keys(results).forEach(function(title,numExo) { %>
            <div class="accordion-item">
              <h2 class="accordion-header" id="flush-heading<%= numExo %>">
                <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#flush-collapse<%= numExo %>" aria-expanded="false" aria-controls="flush-collapse<%= numExo %>">
                  <%= title %>
                </button>
              </h2>
              <div id="flush-collapse<%= numExo %>" class="accordion-collapse collapse" aria-labelledby="flush-heading<%= numExo %>" data-bs-parent="#accordionFlushExample">
                <div class="accordion-body accordion-body-flex" >
                  <% results[title].forEach(function(test,id){ %>
                    <%- include('card.ejs', {test,id}); %>
                    <% }); %>
                  </div>
              </div>
            </div>
          <% }) %>
        </div>
      </div>
      <div>
      </div>
      <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
    </body>
  </html>